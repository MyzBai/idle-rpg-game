{
    "$id": "enemies-schema.json",
	"type": "object",
	"additionalProperties": false,
	"required": ["data"],
	"properties": {
		"$schema": {
			"type": "string"
		},
		"data": {
            "type": "object",
            "required": ["enemyList"],
			"properties": {
				"enemyList": {
					"type": "array",
					"items": {
						"type": "object",
                        "required": ["health"],
                        "properties": {
                            "health": {"type": "number"}
                        }
					}
				}
			}
		}
	}
}
